<body>
    <table id="testResult">
        <thead>
          <tr>
            <th>email</th>
            <th>created</th>
          </tr>
        </thead>
        <tbody>
            <% for( let index = 0; index < emailResult.length; index++ ) { %>
                <tr>
                    <td>
                        <%= emailResult[index] %>
                    </td>
                    <td>
                        <%= createdResult[index] %>
                    </td>
                    <td>
                        <form method="POST" action='/moreInfo'>
                            <input type="text" class="form" name="more" value="<%= emailResult[index] %>" style="display: none;">
                            <button>Info</button>
                        </form>
                    </td>
                </tr>
            <% } %>
            <a href="?page=<%= i-1 %>">&laquo; Previous</a>
            <a href="?page=<%= i+1 %>" style="position: absolute; right: 0;">Next &raquo;</a>
        </tbody>
      </table>

    <form method="POST" action='/register'>
        <input type="text" class="form" name="email" placeholder="Enter Your Email">
        <button>Join Now</button>
    </form>

    <form method="POST" action='/search'>
        <input type="text" class="form" name="search" placeholder="Search for Email">
        <button>search</button>
    </form>

    <form method="POST" action='/replace'>
        <input type="text" class="form" name="replaceFrom" placeholder="Email to be updated">
        <input type="text" class="form" name="replaceTo" placeholder="Updated Email">
        <button>replace</button>
    </form>

    <form method="POST" action='/delete'>
        <input type="text" class="form" name="delete" placeholder="Email to be deleted">
        <button>delete</button>
    </form>


</body>
<!-- //testResult -->
